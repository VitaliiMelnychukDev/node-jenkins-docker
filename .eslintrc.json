{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfline": "auto"
      }
    ]
  },
  "plugins": ["prettier"]
}
